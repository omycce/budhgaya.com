$PROJECT = "advance-block-471015-k4"$TRIGGER_ID = "0e432dc7-8de7-48b5-830f-5d480d11453a"$TOKEN = gcloud auth print-access-token$patchBody = @{  build = @{    options = @{      logging = "CLOUD_LOGGING_ONLY"    }  }} | ConvertTo-Json -Depth 6# PATCH only the build.options portionInvoke-RestMethod -Method Patch `  -Uri "https://cloudbuild.googleapis.com/v1/projects/$PROJECT/triggers/$TRIGGER_ID?updateMask=build" `  -Headers @{ Authorization = "Bearer $TOKEN"; "Content-Type" = "application/json" } `  -Body $patchBody