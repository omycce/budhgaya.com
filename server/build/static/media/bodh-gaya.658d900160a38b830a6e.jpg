function callTopicSlider(parentItem) {
const btnLeft = $(parentItem).find("#swiper-button-prev")[0];
const btnRight = $(parentItem).find("#swiper-button-next")[0];
const container = $(parentItem).find("#slideGroup")[0];
const storyTopcInit = $(parentItem).find(".soryTopcInit")[0];
const containerElementSize = container.querySelector("li").clientWidth || 300;
btnLeft.disabled = true;
btnRight.disabled = true;
btnLeft.onclick = function () {
container.scrollLeft -= containerElementSize;
};
if (container.offsetWidth < storyTopcInit.offsetWidth) {
btnRight.disabled = true;
}
btnRight.onclick = function () {
container.scrollLeft += containerElementSize;
};
container.addEventListener('scroll', function () {
btnLeft.disabled = container.scrollLeft <= 0;
btnRight.disabled = container.offsetWidth + container.scrollLeft >= (container.scrollWidth - 10);
});
// Enable or disable buttons based on initial scroll position
btnLeft.disabled = container.scrollLeft <= 0;
btnRight.disabled = container.offsetWidth + container.scrollLeft >= container.scrollWidth;
}
